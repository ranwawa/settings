<templateSet group="RanWaWa">
  <template name="arrow-inline" value="() =&gt; ({})" description="多行箭头函数" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="TypeScript" value="true" />
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="vuex-mutation" value="// $comment$&#10;set$mutationName$(state, value) {&#10;  state.$stateName$ = value;&#10;}," description="html文件注释" toReformat="false" toShortenFQNames="true">
    <variable name="comment" expression="" defaultValue="&quot;一定要写注释哦&quot;" alwaysStopAt="true" />
